<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    tools:context="pl.damiandziura.kontrolawydatkow.dodajStalyWydatek">

    <TextView
        android:id="@+id/lblczestotliwosc"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:layout_marginRight="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginTop="8dp"
        android:text="@string/str_staly_wydatek"
        app:layout_constraintRight_toLeftOf="@+id/ListViewCzestotliwosc"
        app:layout_constraintTop_toTopOf="@+id/ListViewCzestotliwosc" />

    <ExpandableListView
        android:id="@+id/ListViewCzestotliwosc"
        android:layout_width="177dp"
        android:layout_height="29dp"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="9dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDatapierwszego"
        tools:layout_editor_absoluteY="356dp" />

    <TextView
        android:id="@+id/lblDo"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:layout_marginRight="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginTop="0dp"
        android:text="@string/str_do"
        app:layout_constraintRight_toLeftOf="@+id/txtDo"
        app:layout_constraintTop_toTopOf="@+id/txtDo" />

    <TextView
        android:id="@+id/lblDatapierwszego"
        android:layout_width="wrap_content"
        android:layout_height="23dp"
        android:layout_marginRight="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginTop="0dp"
        android:text="@string/str_data_pobrania"
        app:layout_constraintRight_toLeftOf="@+id/txtDatapierwszego"
        app:layout_constraintTop_toTopOf="@+id/txtDatapierwszego" />

    <TextView
        android:id="@+id/lblOd"
        android:layout_width="22dp"
        android:layout_height="23dp"
        android:layout_marginEnd="0dp"
        android:text="@string/str_od"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toBottomOf="@+id/ListViewKategoria"
        app:layout_constraintRight_toLeftOf="@+id/txtOd"
        android:layout_marginRight="0dp" />

    <EditText
        android:id="@+id/txtDatapierwszego"
        android:layout_width="114dp"
        android:layout_height="23dp"
        android:ems="10"
        android:inputType="date"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="@+id/ListViewPodkategoria"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtDo" />

    <EditText
        android:id="@+id/txtDo"
        android:layout_width="114dp"
        android:layout_height="23dp"
        android:ems="10"
        android:inputType="date"
        android:layout_marginRight="17dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="@+id/ListViewPodkategoria"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toBottomOf="@+id/ListViewPodkategoria" />

    <EditText
        android:id="@+id/txtOd"
        android:layout_width="114dp"
        android:layout_height="23dp"
        android:ems="10"
        android:inputType="date"
        android:layout_marginEnd="7dp"
        app:layout_constraintTop_toTopOf="@+id/lblOd"
        android:layout_marginTop="0dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/ListViewKategoria" />

    <EditText
        android:id="@+id/txtKwota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/lblKwota"
        android:layout_marginLeft="0dp"
        android:layout_marginStart="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/lblKwota" />

    <TextView
        android:id="@+id/lblKwota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/str_kwota"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="84dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btDodaj"
        android:layout_width="322dp"
        android:layout_height="83dp"
        android:layout_marginRight="77dp"
        android:layout_marginStart="16dp"
        android:onClick="dodaj"
        android:text="@string/str_add"
        app:layout_constraintRight_toRightOf="@+id/btWyczysc"
        android:layout_marginTop="-159dp"
        app:layout_constraintTop_toBottomOf="@+id/ListViewCzestotliwosc" />

    <Button
        android:id="@+id/btWyczysc"
        android:layout_width="157dp"
        android:layout_height="83dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:onClick="wyczysc"
        android:text="@string/str_clear"
        app:layout_constraintLeft_toRightOf="@+id/btCofnij"
        app:layout_constraintTop_toTopOf="@+id/btCofnij"
        android:layout_marginTop="0dp" />

    <Button
        android:id="@+id/btCofnij"
        android:layout_width="157dp"
        android:layout_height="83dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:onClick="cofnij"
        android:text="@string/stre_back"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/btDodaj" />

    <ExpandableListView
        android:id="@+id/ListViewPodkategoria"
        android:layout_width="165dp"
        android:layout_height="120dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintLeft_toRightOf="@+id/ListViewKategoria"
        app:layout_constraintTop_toTopOf="@+id/ListViewKategoria" />

    <ExpandableListView
        android:id="@+id/ListViewKategoria"
        android:layout_width="165dp"
        android:layout_height="120dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/txtKwota"
        app:layout_constraintTop_toBottomOf="@+id/txtKwota" />

    <TextView
        android:id="@+id/lblStalyWydatekNazwa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_nazwaa"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="1dp" />
</android.support.constraint.ConstraintLayout>
